<!DOCTYPE HTML>
<html>
<head>
<meta charset="utf-8">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="jsmol/JSmol.min.js"></script>
</head>

<body>

  <h1> Model: <%= data.userQuery %></h1>
  <p> Number of domains: <%= data.searchResults.length %> </p>


  <table>
    <tr>
      <th>Domain</th>
      <th>Start</th>
      <th>End</th>
      <th>Color</th>
    </tr>
  
    <% for (var i = 0; i < data.searchResults.length; i++) { %>
      <tr>
        <td><%= data.searchResults[i].Domain %></td>
        <td><%= data.searchResults[i].Start %></td>
        <td><%= data.searchResults[i].End %></td>
        <td><%= data.searchResults[i].Color %></td>
      </tr>    
    <% } %>
  </table>

<!-- <img src="colbydom.jpg" /> -->
  <script type="text/javascript">
      
    $(document).ready(function(){
        var Info = {
            color: "#FFFFFF",
            height: 600,
            width: 600,
            script: "myJmolscript.spt",
            use: "HTML5",
            j2sPath: "jsmol/j2s",
            serverURL: "jsmol/php/jsmol.php",
        };
        $("#somePlace").html(Jmol.getAppletHtml("myJmol", Info));        
    });
  </script>

  <h2>Visualization</h2>
  <div id="somePlace"></div>

</body>
</html>

